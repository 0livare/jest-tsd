  "peerDependencies": {
    "@tsd/typescript": "4.x || 5.x"
  },
  "dependencies": {
    "@babel/code-frame": "^7.21.4",
    "chalk": "^4.1.2",
    "find-up": "^6.3.0",
    "pkg-dir": "^7.0.0",
    "tsd-lite": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/babel__code-frame": "^7.0.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "keywords": [
